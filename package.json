{
  "name": "dot-me",
  "private": true,
  "description": "Personal site and portfolio for @fsvdr (a.k.a me)",
  "version": "1.0.0",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "framer-motion": "^1.11.0",
    "gatsby": "^2.19.7",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sharp": "^2.6.24",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-styled-components": "^3.3.4",
    "gatsby-remark-vscode": "^3.0.1",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-remark": "^2.8.27",
    "gatsby-transformer-sharp": "^2.5.12",
    "horizon-theme-vscode": "jolaleye/horizon-theme-vscode",
    "netlify-lambda": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-prettier": "^1.1.2"
  },
  "keywords": [
    "fsvdr",
    "web developer",
    "front end",
    "portfolio"
  ],
  "license": "MIT",
  "scripts": {
    "postinstall": "netlify-lambda install",
    "develop": "gatsby develop -H 0.0.0.0",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:css": "stylelint \"**/*.css\" --ignore-path .gitignore",
    "prettier": "prettier \"README.md\" \"gatsby-*.js\" \"src/**/*.{js,jsx,json,md,css}\" --ignore-path .gitignore",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "npm-run-all --parallel lint:js lint:css build",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "volta": {
    "node": "12.18.3",
    "yarn": "1.22.0"
  }
}
